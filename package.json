{
  "name": "jest-matcher-http",
  "version": "0.0.0-development",
  "description": "Additional Jest matchers for HTTP responses.",
  "main": "src/index.js",
  "engines": {
    "node": ">=20"
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "jest --config jest.unit.config.js",
    "test:unit:coverage": "jest --coverage --config jest.unit.config.js",
    "test": "npm run test:unit:coverage",
    "semantic-release": "semantic-release --branches main",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rimesime/jest-matcher-http.git"
  },
  "author": "Simon Schmitt (mail@simonschmitt.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rimesime/jest-matcher-http/issues"
  },
  "homepage": "https://github.com/rimesime/jest-matcher-http#readme",
  "release": {
    "branches": [
      "main"
    ],
    "repositoryUrl": "git+ssh://git@github.com/rimesime/jest-matcher-http.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@eslint/js": "^9.39.2",
    "@types/jest": "^29.5.12",
    "axios": "^1.3.4",
    "eslint": "^9.39.2",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jsdoc": "^62.5.0",
    "express": "^4.18.2",
    "globals": "^17.2.0",
    "http-terminator": "^3.2.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "needle": "^3.2.0",
    "semantic-release": "^25.0.3",
    "signal-promise": "^1.0.3",
    "superagent": "^8.0.9",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@jest/globals": "^29.5.0"
  },
  "overrides": {
    "superagent": {
      "formidable": "^3.2.4"
    },
    "tar": "^7.5.7"
  }
}
